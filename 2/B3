row, post = map(int, input().split())
a = []
for i in range(row):
    r = list(map(int, input().split()))
    a.append(r)
m = len(a)
n = len(a[0])
at = [[0 for _ in range(m)] for _ in range(n)]
for i in range(m):
    for j in range(n):
        at[j][i] = a[i][j]
for r in at:
    print(" ".join(map(str, r)))

